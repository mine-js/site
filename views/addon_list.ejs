<%- include('header') %>
<span style="font-size: 48px; font-weight: bolder;">Addons</span>
<br/>

<a class="btn btn-primary" href="/addon/upload" role="button">Upload your own addon</a>

<br/><br/><br/>


<% addons.forEach((elem) => { %>
    <div class="card">
      <div class="card-body">
        <a href="/addon/info/<%= elem.id %>" style="text-decoration: none; color: white;"><h5 class="card-title"><%= elem.name %> (<%= elem.id %>)</h5></a>
        <p class="card-text"><small class="text-muted">Latest: <%= elem.latest %></small></p>
      </div>
    </div><br/>
<% }) %>
<%- include('footer') %>